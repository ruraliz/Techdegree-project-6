extends layout.pug

block content
  section.desk
    h1= `Server Error`
    h2= `Error status code: ${err.status} - Server Error`
    p.error-p= `Message: ${err.message}`
    img.desk-items(src='/static/images/desk.svg')
    a.button-primary-small(href='/') Go Home &rarr;
    .paper
      pre.stack-trace= `${err.stack}`
    .tear